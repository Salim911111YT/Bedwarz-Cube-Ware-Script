			local shouldAllowGlassBridgeThing = true

			plr.Chatted:Connect(function(msg)
				local msgSplit = msg:split(" ")

				if msg:find("/spawn") then
					if tostring(msgSplit[1] ~= nil) and tostring(msgSplit[2]) ~= nil and tostring(msgSplit[3]) ~= nil then
						if tostring(msgSplit[2]) ~= nil and typeof(tonumber(msgSplit[3])) == "number" then
							local module = require(game:GetService("ReplicatedStorage"):WaitForChild("Modules"):WaitForChild("InventoryModule"))
							local items = {
								["Wool"] = "Blocks";
								["Obsidian"] = "Blocks";
								["Blastproof Ceramic"] = "Blocks";
								["Stone Brick"] = "Blocks";
								["Wood Plank"] = "Blocks";
								["TNT"] = "Blocks";
								["Slime"] = "Blocks";
								["Iron"] = "Ressources";
								["Diamond"] = "Ressources";
								["Emerald"] = "Ressources";
							}

							module.AddItem(tostring(msgSplit[2]), tonumber(msgSplit[3]), items[tostring(msgSplit[2])])
						end
					end
				elseif msg == "/see glass_bridge" then
					for _, v in pairs(workspace:WaitForChild("GlassPads"):GetDescendants()) do
						if v:IsA("Model") then
							if v:GetAttribute("IsSafe") == false then
