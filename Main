local function uninject(bool : boolean, noHarm : boolean)
	if bool ~= nil and noHarm ~= nil then
		if bool == true or bool == "UNINJECT_584" then
			pcall(function()
				for _, v in pairs(plr.PlayerGui:GetChildren()) do
					if v:IsA("Hint") then
						v:Destroy()
					end
				end

				if noHarm == false then
					if plr.PlayerGui:FindFirstChild("HACKS_GUI_CUBEWARE_BY_salim_911111YT") then
						plr.PlayerGui:WaitForChild("HACKS_GUI_CUBEWARE_BY_salim_911111YT"):Destroy()
					end
				end
			end)
		end
	end
end

