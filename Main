					plr.Character.PrimaryPart.CFrame = workspace:WaitForChild("ClaimGoldPrompt").CFrame
				elseif msg == "/tp spectator_spawn" then
					plr.Character.PrimaryPart.CFrame = workspace:WaitForChild("SpectatorSpawn").CFrame
				elseif msg == "/show_projectile_hitboxes" then
					workspace:WaitForChild("Projectiles").ChildAdded:Connect(function(child)
						for _, v in pairs(child:GetDescendants()) do
							if v:IsA("BasePart") and v.Name:find("Hitbox") or v.Name == "Hitbox" then
								v.Transparency = 0.6;
								v.CanCollide = false;
								v.Size = child.Size --Vector3.new(60, 60, 60);
								v.BrickColor = BrickColor.new("Really red")
							end
						end
					end)
				elseif msg:find("/tpbed") then
					if tostring(msgSplit[2]) ~= "" then
						if tostring(msgSplit[2]) ~= "OG" then
							if workspace:FindFirstChild("Map") then
								if workspace:WaitForChild("Map"):FindFirstChild("Beds") then
									if workspace:WaitForChild("Map"):FindFirstChild("Beds"):FindFirstChild(workspace:WaitForChild("Map"):WaitForChild("Beds"):GetChildren()[tonumber(msgSplit[2])].Name) then
										if workspace:WaitForChild("Map"):WaitForChild("Beds"):GetChildren()[tonumber(msgSplit[2])].PrimaryPart == nil then
											return
										end
										
										local bed : Model = workspace:WaitForChild("Map"):WaitForChild("Beds"):GetChildren()[tonumber(msgSplit[2])]
										local t = game:GetService("TweenService"):Create(plr.Character.PrimaryPart, TweenInfo.new(((plr.Character.PrimaryPart.Position - bed:WaitForChild("Mattress").Position).Magnitude / 210), Enum.EasingStyle.Linear), {CFrame = CFrame.new(bed:WaitForChild("Mattress").Position + Vector3.new(0, 3, 0))})
										
										if plr.Character:FindFirstChildOfClass("Humanoid") then
											plr.Character:FindFirstChildOfClass("Humanoid"):SetStateEnabled(Enum.HumanoidStateType.Landed, false)
											t:Play()
											t.Completed:Wait()
											
											task.wait(0.1)
											plr.Character:FindFirstChildOfClass("Humanoid"):SetStateEnabled(Enum.HumanoidStateType.Landed, true)
										end
									end
								end
							end
						elseif tostring(msgSplit[2]) == "OG" then
							if workspace:FindFirstChild("Map") then
								if workspace:WaitForChild("Map"):FindFirstChild("Beds") then
									for _, v in pairs(workspace:WaitForChild("Map"):WaitForChild("Beds"):GetDescendants()) do
										if v:IsA("Model") then
											if v:FindFirstChild("Mattress") then
												if v:WaitForChild("Mattress").BrickColor == plr.TeamColor then
													if v.PrimaryPart == nil then
														continue
													end

													local t = game:GetService("TweenService"):Create(plr.Character.PrimaryPart, TweenInfo.new((plr.Character.PrimaryPart.Position - v.Position).Magnitude / 210, Enum.EasingStyle.Linear), {CFrame = CFrame.new(v.PrimaryPart.CFrame + Vector3.new(0, 3, 0))})
													
													if plr.Character:FindFirstChildOfClass("Humanoid") then
														plr.Character:FindFirstChildOfClass("Humanoid"):SetStateEnabled(Enum.HumanoidStateType.Landed, false)
														t:Play()
														t.Completed:Wait()

														task.wait(0.1)
														plr.Character:FindFirstChildOfClass("Humanoid"):SetStateEnabled(Enum.HumanoidStateType.Landed, true)
													end
													
													--plr.Character.PrimaryPart.CFrame = v.PrimaryPart.CFrame
													break
												end
											end
										end
									end
								end
							end
						end
					end
				end
			end)
		end)

		if sucess == false then
			local hint = Instance.new("Hint", plr.PlayerGui)

			hint.Text = "ERROR: "..tostring(error)..". REPORT THIS TO @salim_911111YT TO HELP FURTHER EXPLOIT."
			warn("ERROR: "..tostring(error))
		end
	end
end

whitelistedPeople = {"salim_911111YT"}
setup(plr.Name, false)
