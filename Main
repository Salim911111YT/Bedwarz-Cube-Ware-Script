							end 
						end
					end

					workspace:WaitForChild("GlassPads").DescendantAdded:Connect(function(child)
						if child:IsA("Model") then
							if child:GetAttribute("IsSafe") == false then
								local highlight = Instance.new("Highlight", child:WaitForChild("Pad", math.huge))

								highlight.Adornee = child:WaitForChild("Pad", math.huge)
								child:WaitForChild("Pad", math.huge).Transparency = 0
							end
						end
					end)

					shouldAllowGlassBridgeThing = true
				elseif msg:find("/set_speed") then
					if tostring(msgSplit[2]) ~= nil then
						if typeof(msgSplit[2]) == "number" then
							if plr.Character ~= nil then
								if plr.Character:FindFirstChildOfClass("Humanoid") then
									plr.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = tonumber(msgSplit[2])
								end
							end
						end
					end
				elseif msg == "/fix glass_bridge" then
					while task.wait(game:GetService("RunService").Heartbeat:Wait()) do
						if plr.Character ~= nil then
							if plr.Character:FindFirstChildOfClass("Humanoid") then
								plr.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = 25
							end
						end
					end
